{
  "name": "@protokoll/core",
  "description": "Protokoll Core Functionality",
  "author": "Martin Auer",
  "version": "0.2.11",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "build": "pkgroll",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test src/**/*.test.ts"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@protokoll/eslint-config": "workspace:*",
    "@protokoll/prettier-config": "workspace:*",
    "@protokoll/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jose": "catalog:",
    "msw": "catalog:",
    "pkgroll": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "type-fest": "^4.24.0",
    "typescript": "catalog:"
  },
  "prettier": "@protokoll/prettier-config"
}
