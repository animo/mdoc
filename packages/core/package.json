{
  "name": "@protokoll/core",
  "description": "Protokoll Core Functionality",
  "author": "Martin Auer",
  "version": "0.2.17",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "build": "pkgroll --sourcemap",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test src/**/*.test.ts"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@protokoll/eslint-config": "workspace:*",
    "@protokoll/prettier-config": "workspace:*",
    "@protokoll/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jose": "catalog:",
    "msw": "catalog:",
    "pkgroll": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "type-fest": "^4.24.0",
    "typescript": "catalog:"
  },
  "prettier": "@protokoll/prettier-config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protokoll/packages/core"
  }
}
