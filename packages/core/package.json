{
  "name": "@protokoll/core",
  "description": "Protokoll Core Functionality",
  "author": "Martin Auer",
  "version": "0.2.30",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/esm/src/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --module commonjs --moduleResolution node --verbatimModuleSyntax false --outDir dist/cjs",
    "build:esm": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test src/**/*.test.ts"
  },
  "dependencies": {
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@protokoll/eslint-config": "workspace:*",
    "@protokoll/prettier-config": "workspace:*",
    "@protokoll/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "type-fest": "^4.26.1",
    "typescript": "catalog:"
  },
  "prettier": "@protokoll/prettier-config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protokoll/packages/core"
  }
}
