{
  "name": "@protokoll/mdoc-client",
  "description": "Protokoll Mdoc and MDL client",
  "author": "Martin Auer",
  "version": "0.2.34",
  "private": false,
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/cjs/src/index.js",
  "types": "./dist/cjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cjs/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/cjs/src/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "build": "pnpm build:cjs",
    "build:cjs": "tsc --module commonjs --moduleResolution node --verbatimModuleSyntax false --outDir dist/cjs",
    "format": "prettier --check . --ignore-path ../../../.gitignore",
    "lint": "echo 'TODO: FIX AND ENABLE'",
    "typecheck": "tsc --noEmit --verbatimModuleSyntax false"
  },
  "dependencies": {
    "@protokoll/core": "workspace:*",
    "compare-versions": "^6.1.1"
  },
  "devDependencies": {
    "@ausweis/eslint": "catalog:",
    "@ausweis/prettier": "catalog:",
    "@ausweis/typescript": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jose": "^5.9.3",
    "prettier": "catalog:",
    "rollup": "^4.24.2",
    "typescript": "catalog:"
  },
  "prettier": "@ausweis/prettier",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protokoll/packages/mdoc/mdoc-client"
  },
  "keywords": [
    "auth0",
    "mdl",
    "iso-18013-5",
    "iso-18013-7",
    "m-doc",
    "m-doc-cbor",
    "m-doc-cbor-parser",
    "m-doc-cbor-validator",
    "m-doc-cbor-encoder",
    "m-doc-cbor-decoder",
    "m-doc-cbor-iso-18013-5",
    "m-doc-cbor-iso-18013-5-parser",
    "m-doc-cbor-iso-18013-5-validator",
    "m-doc-cbor-iso-18013-5-encoder",
    "m-doc-cbor-iso-18013-5-decoder"
  ]
}
