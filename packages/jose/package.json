{
  "name": "@protokoll/jose",
  "description": "Protokoll Jose",
  "author": "Martin Auer",
  "private": false,
  "version": "0.2.17",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "build": "pkgroll --sourcemap",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test src/**/*.test.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./u-jose-test-context": {
      "require": {
        "types": "./dist/u-jose-context.d.cts",
        "default": "./dist/u-jose-context.cjs"
      },
      "import": {
        "types": "./dist/u-jose-context.d.mts",
        "default": "./dist/u-jose-context.mjs"
      }
    }
  },
  "dependencies": {
    "@protokoll/core": "workspace:*",
    "jwt-decode": "^4.0.0",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@protokoll/eslint-config": "workspace:*",
    "@protokoll/prettier-config": "workspace:*",
    "@protokoll/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "jose": "catalog:",
    "msw": "catalog:",
    "pkgroll": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@protokoll/prettier-config",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protokoll/packages/jose"
  }
}
