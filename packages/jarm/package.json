{
  "name": "@protokoll/jarm",
  "description": "Protokoll JARM (JWT Secured Authorization Response Mode)",
  "author": "Auer Martin",
  "version": "0.2.32",
  "private": false,
  "type": "module",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js",
      "types": "./dist/esm/src/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "git clean -xdf .cache .turbo node_modules dist",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc --module commonjs --moduleResolution node --verbatimModuleSyntax false --outDir dist/cjs",
    "build:esm": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "tsx --test src/**/*.test.ts"
  },
  "dependencies": {
    "@protokoll/core": "workspace:*",
    "@protokoll/jose": "workspace:*",
    "valibot": "catalog:"
  },
  "devDependencies": {
    "@ausweis/eslint": "catalog:",
    "@ausweis/prettier": "catalog:",
    "@ausweis/typescript": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "msw": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@ausweis/prettier",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/protokoll/packages/jarm"
  }
}
